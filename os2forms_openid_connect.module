<?php

/**
 * @file
 * Contains os2forms_openid_connect.module.
 */

use Drupal\Core\Form\FormStateInterface;
use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function os2forms_openid_connect_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the os2forms_openid_connect module.
    case 'help.page.os2forms_openid_connect':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('OS2Forms OpenID Connect') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_webform_third_party_settings_form_alter().
 */
function os2forms_openid_connect_webform_third_party_settings_form_alter(&$form, FormStateInterface $form_state) {
  Drupal::service('os2forms_openid_connect.form_helper')->webformThirdPartySettingsFormAlter($form, $form_state);
}
